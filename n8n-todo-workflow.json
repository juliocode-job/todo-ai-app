      "main": [
        [{"node": "List Todos"}],
        [{"node": "Create Todo"}],
        [{"node": "List for Complete"}],
        [{"node": "Delete Todo"}],
        [{"node": "Help Message"}]
      ]
    },
    "List Todos": {
      "main": [[{"node": "Send Response"}]]
    },
    "Create Todo": {
      "main": [[{"node": "Send Response"}]]
    },
    "List for Complete": {
      "main": [[{"node": "Get Todo ID"}]]
    },
    "Get Todo ID": {
      "main": [[{"node": "Complete Todo"}]]
    },
    "Complete Todo": {
      "main": [[{"node": "Send Response"}]]
    },
    "Delete Todo": {
      "main": [[{"node": "Send Response"}]]
    },
    "Help Message": {
      "main": [[{"node": "Send Response"}]]
